name: dockercompose13243466776656029076
services:
  travels.api:
    build:
      context: C:\Users\julio\OneDrive\Documentos\TravelAgency\CRUD12_Julio_Pech\Travels.Api
      dockerfile: Travels.Api/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
