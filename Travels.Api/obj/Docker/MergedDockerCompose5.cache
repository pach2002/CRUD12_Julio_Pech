        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  travels.mysql:
    container_name: travels.mysql
    environment:
      MYSQL_DATABASE: travels
      MYSQL_ROOT_PASSWORD: "123456"
    image: mysql:8.0.36-oracle
    networks:
      default: null
    ports:
      - mode: ingress
        target: 3306
        published: "18004"
        protocol: tcp
    volumes:
      - type: volume
        source: registers
        target: /data/db
        volume: {}
